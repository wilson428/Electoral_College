Cell[BoxData[{
 StyleBox[
  RowBox[{
   RowBox[{"csv", " ", "=", " ", 
    RowBox[{"Import", "[", 
     RowBox[{
     "\"\<https://www2.census.gov/programs-surveys/decennial/tables/2010/2010-\
apportionment/pop_change.csv\>\"", ",", " ", "\"\<CSV\>\""}], "]"}]}], ";"}],
  FontSize->12], "\n", 
 RowBox[{
  RowBox[{"data", " ", "=", " ", 
   RowBox[{"Most", "@", 
    RowBox[{"Take", "[", 
     RowBox[{"csv", ",", " ", 
      RowBox[{"-", "52"}]}], "]"}]}]}], ";"}], "\n", 
 RowBox[{"header", " ", "=", " ", 
  RowBox[{"csv", "[", 
   RowBox[{"[", "3", "]"}], "]"}]}]}], "Code",
 CellChangeTimes->{{3.7431812189576197`*^9, 3.743181247077299*^9}, 
