Cell["\<\
Let's toss in entities to the output, since we can be sure they\
\[CloseCurlyQuote]re uniform across different sources, spellings and \
punctuations\
\>", "Text",
 CellChangeTimes->{{3.7431801353864393`*^9, 
  3.743180186497472*^9}},ExpressionUUID->"6cccf251-830f-43da-ab81-\
d55bf7dc47fe"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"data", " ", "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"{", " ", 
      RowBox[{
       RowBox[{"StringTrim", "[", 
        RowBox[{"First", "@", "#"}], "]"}], ",", " ", 
       RowBox[{"StringTrim", "[", 
        RowBox[{"Last", "@", "#"}], "]"}]}], " ", "}"}], "&"}], " ", "/@", 
    " ", 
    RowBox[{"Rest", "@", "table"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"data", " ", "=", " ", 
   RowBox[{"Select", "[", 
    RowBox[{"data", ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"First", "@", "#"}], " ", "\[NotEqual]", " ", "\"\<\>\""}], 
       " ", "&&", " ", 
       RowBox[{
        RowBox[{"First", "@", "#"}], " ", "\[NotEqual]", " ", 
        "\"\<Puerto Rico\>\""}]}], "&"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"states", " ", "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"<|", " ", 
      RowBox[{
       RowBox[{"\"\<name\>\"", " ", "\[Rule]", " ", 
        RowBox[{"First", "@", "#"}]}], ",", " ", 
       RowBox[{"\"\<abbr\>\"", " ", "\[Rule]", " ", 
        RowBox[{"Last", "@", "#"}]}], ",", " ", 
       RowBox[{"\"\<entity\>\"", " ", "\[Rule]", " ", 
        RowBox[{
         RowBox[{"Interpreter", "[", "\"\<USState\>\"", "]"}], "[", 
         RowBox[{"First", "@", "#"}], "]"}]}]}], " ", "|>"}], "&"}], " ", "/@",
     " ", "data"}]}], ";"}]}], "Code",
 CellLabel->
  "In[242]:=",ExpressionUUID->"31c7b19f-e338-42cc-8dcd-6ea823a7447f"],

