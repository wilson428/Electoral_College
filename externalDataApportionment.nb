Cell["\<\
We\[CloseCurlyQuote]ll start by converting the full state names to entities, \
since we can be sure they\[CloseCurlyQuote]re uniform across different \
sources and punctuation\
\>", "Text",
 CellChangeTimes->{{3.7431801353864393`*^9, 
  3.743180186497472*^9}},ExpressionUUID->"6cccf251-830f-43da-ab81-\
d55bf7dc47fe"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"data", " ", "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"{", " ", 
      RowBox[{
       RowBox[{"StringTrim", "[", 
        RowBox[{"First", "@", "#"}], "]"}], ",", " ", 
       RowBox[{"StringTrim", "[", 
        RowBox[{"Last", "@", "#"}], "]"}]}], " ", "}"}], "&"}], " ", "/@", 
    " ", 
    RowBox[{"Rest", "@", "table"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"data", " ", "=", " ", 
   RowBox[{"Select", "[", 
    RowBox[{"data", ",", " ", 
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{"First", "@", "#"}], " ", "\[NotEqual]", " ", "\"\<\>\""}], 
       " ", "&&", " ", 
       RowBox[{
        RowBox[{"First", "@", "#"}], " ", "\[NotEqual]", " ", 
        "\"\<Puerto Rico\>\""}]}], "&"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"states", " ", "=", " ", 
   RowBox[{
    RowBox[{
     RowBox[{"<|", " ", 
      RowBox[{
       RowBox[{"\"\<name\>\"", " ", "\[Rule]", " ", 
        RowBox[{"First", "@", "#"}]}], ",", " ", 
       RowBox[{"\"\<abbr\>\"", " ", "\[Rule]", " ", 
        RowBox[{"Last", "@", "#"}]}], ",", " ", 
       RowBox[{"\"\<entity\>\"", " ", "\[Rule]", " ", 
        RowBox[{
         RowBox[{"Interpreter", "[", "\"\<USState\>\"", "]"}], "[", 
         RowBox[{"First", "@", "#"}], "]"}]}]}], " ", "|>"}], "&"}], " ", "/@",
     " ", "data"}]}], ";"}]}], "Code",
 CellChangeTimes->{
  3.743180461164468*^9, {3.7431805111069717`*^9, 3.7431807566590347`*^9}, {
   3.743180820600212*^9, 3.743180890615374*^9}, {3.743180939158474*^9, 
   3.7431809401969357`*^9}},
 CellLabel->
  "In[1660]:=",ExpressionUUID->"28baf75e-ca77-4ec4-a069-87f47635d425"],

